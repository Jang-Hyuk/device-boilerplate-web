{
  "name": "device-boilerplate-web",
  "version": "0.0.1",
  "private": true,
  "bin": {
    "app": "./bin/www.js"
  },
  "pkg": {
    "assets": [
      "node_modules/nanoid/**/*",
      "docs/**/*",
      "views/**/*",
      "maps/**/*",
      "public/**/*"
    ]
  },
  "scripts": {
    "start": "nodemon --trace-warnings --ignore public/* ./bin/www.js",
    "build": "pkg . --out-path dist",
    "build:win": "pkg . --out-path dist --targets node8-win-x64",
    "build:linux": "pkg . --out-path dist --targets node8-linux-x64",
    "build:macos": "pkg . --out-path dist --targets node8-macos-x64",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'public/**/*.{css,scss}' ",
    "forever": "forever start --minUptime 5000 --spinSleepTime 2000 -a bin/www.js"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.167",
    "docdash": "^1.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "jsdoc": "^3.6.6",
    "pkg": "^4.4.9",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "base-model-jh": "^1.2.9",
    "base-util-jh": "^1.3.3",
    "base64-img": "^1.0.4",
    "body-parser": "^1.19.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "debug": "^4.3.1",
    "device-protocol-converter-jh": "^0.1.21",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-mysql-session": "^2.1.5",
    "express-session": "^1.17.1",
    "helmet": "^4.3.1",
    "http-errors": "^1.8.0",
    "lodash": "^4.17.20",
    "method-override": "^3.0.0",
    "moment": "^2.29.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "request": "^2.88.2",
    "rtsp-ffmpeg": "0.0.15",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.3.0",
    "split": "^1.0.1",
    "uuid": "^8.3.2",
    "weather-cast": "^0.3.1",
    "ws": "^7.4.2",
    "xlsx": "^0.16.9",
    "xss": "^1.0.8"
  }
}
