{
  "name": "device-boilerplate-web",
  "version": "0.0.0",
  "private": true,
  "bin": {
    "app": "./bin/www.js"
  },
  "pkg": {
    "assets": [
      "views/**/*",
      "public/**/*"
    ]
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "build": "pkg . --out-path dist",
    "build:win": "pkg . --out-path dist --targets node8-win-x64",
    "build:linux": "pkg . --out-path dist --targets node8-linux-x64",
    "build:macos": "pkg . --out-path dist --targets node8-macos-x64",
    "fix-code": "prettier-eslint --write 'src/**/*.{js,jsx}' ",
    "fix-styles": "prettier-stylelint --write 'public/**/*.{css,scss}' "
  },
  "devDependencies": {
    "@types/lodash": "^4.14.118",
    "docdash": "^1.0.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "jsdoc": "^3.5.5",
    "pkg": "^4.3.7",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "base-model-jh": "^1.2.5",
    "base-util-jh": "^1.2.1",
    "base64-img": "^1.0.4",
    "body-parser": "^1.18.3",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cron": "^1.6.0",
    "debug": "^4.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-async-handler": "^1.1.4",
    "express-mysql-session": "^2.1.0",
    "express-session": "^1.15.6",
    "http-errors": "^1.7.2",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "mysql": "^2.16.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "request": "^2.88.0",
    "rtsp-ffmpeg": "0.0.14",
    "serve-favicon": "^2.5.0",
    "socket.io": "^2.2.0",
    "split": "^1.0.1",
    "uuid": "^3.3.2",
    "weather-cast": "^0.2.3",
    "ws": "^6.1.4",
    "xlsx": "^0.14.1"
  }
}
